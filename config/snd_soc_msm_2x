SectionDevice
	Name "Earpiece"
	Comment "Handset Rx device"

	EnableSequence
		'SLIM_0_RX Channels':0:One
		'RX1 MIX1 INP1':0:RX1
		'EAR PA Gain':0:POS_6_DB
		'DAC1 Switch':1:1
	EndSequence

	DisableSequence
		'RX1 MIX1 INP1':0:ZERO
		'DAC1 Switch':1:0
	EndSequence

	ACDBID	7:1

EndSection

